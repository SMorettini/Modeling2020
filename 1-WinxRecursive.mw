<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">derive_spline := proc(_spline, T, degree) 
  local L, i;
  L := convert(_spline, list);
  L:=piecewise(seq([L[2*i - 1], diff(L[2*i], `$`(T,degree))][], i = 1 .. floor(1/2*nops(L))), diff(L[-1], `$`(T,degree))); 
  return L:
end:</Text-field>
